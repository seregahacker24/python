import keyboard
pole = [[" ", " ", " ", " ", " "],
        [" ", " ", " ", " ", " "],
        [" ", " ", " ", " ", " "],
        [" ", " ", " ", " ", " "],
        [" ", " ", " ", " ", " "],
        [" ", " ", " ", " ", " "],
        [" ", " ", "0 0", " ", " "],
        [" ", " ", "___", " ", " "]]
for stroka in pole:
    for kletka in stroka:
        print(kletka, end = ' ')
    print()
    if keyboard.is_pressed('D'):
        print("ьытдялюьб")
