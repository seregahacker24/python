import random
z = 111
def generation():
    while True:
        a = z % 100
        b = z // 10
        c = b % 10
        d = z // 100
        e = a % 10
        if (c == d) or (c == e) or (e == d):
            z = random.randint(100, 999)
        else:
            print(z)
            break
def proverka():
    x = int(input)
    if x == z:
        print("Ура, ты выиграл, тоесть отгадал число")
