import random
a = random.randint(1, 999)
if a > 99:
    b = a // 10
    c = a % 10
    d = b // 10
    e = b % 10
    x = c + d + e
    y = c * d * e
    print(a, x, y)
elif a < 100 and a > 9:
    b1 = a // 10
    c1 = a % 10
    x1 = b1 + c1
    y1 = b1 * c1
    print(a, x1, y1)
elif a < 10:
    x2 = a
    y2 = a
    print(a, x2, y2)
